a, a:visited, a span{
  color: {{ settings.link_color }};
  text-decoration: none;
  position: relative;
  transition: color .1s linear;
}
a:hover, a:focus, a:active{
  color: {{ settings.link_hover_color }};
  outline:0;
}
.clearfix:after{
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
