.page-helpfaq {
  $transition-timing : linear!important;
  $question-color : #f5a74e;
  $transition-duration : 1s!important;
  .flex-container{
    display: flex;
    flex-direction: row ;
  }
  .flex-item{
    flex: 50%;
    padding-left: 1%;
    padding-right:1%;
  }
  .title-faq{
    text-align: center;
    font-weight: bold;
  }
  .tab-toggle, .subtab-toggle{
    display: none!important;
  }
  .tab-label{
    display: block;
    padding: 10px;
    font-size: 17px;
    font-weight: bold;
  }
  .tab-label::before{
    content:"\002B";
    float: left;
    margin-right: 10px;
    transition: content $transition-duration;
    transition-timing-function: $transition-timing;
  }
  .tab-content{
    max-height: 0px;
    overflow: hidden;
    transition: max-height $transition-duration;
    transition-timing-function: $transition-timing;
  }
  .tab-toggle:checked ~ .tab-label::before{
    content: "\2212";  
    font-size: 17px;    
    transition: content $transition-duration;
    transition-timing-function: $transition-timing;  
  }
  .tab-toggle:checked ~ .tab-content{
    max-height: 100vh;
    transition: max-height $transition-duration;
    transition-timing-function: $transition-timing;
  }
  .subtab-label{
    display: block;
    padding: 3px;
    font-size: 14px;
    font-weight: bold;
    color: $question-color!important;
    margin-left: 10px;
  }
  .subtab-label::before{
    content:"\002B";
    float: left;
    margin-right: 10px;
    transition: content $transition-duration;
    transition-timing-function: $transition-timing;
  }
  .subtab-content{
    max-height: 0px;
    overflow: hidden;
    padding-left: 15px!important;
    transition-timing-function: $transition-timing;
  }
  .subtab-toggle:checked ~ .subtab-label::before{
    content: "\2212";
    transition: content $transition-duration;
    transition-timing-function: $transition-timing;
  }
  .subtab-toggle:checked ~ .subtab-content{
    max-height: 100vh;
    margin-left: 20px;
    padding-top: 10px;
    padding-bottom: 10px;
    transition: max-height $transition-duration;
    transition-timing-function: $transition-timing;
  }
}
