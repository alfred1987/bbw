.countdown_timer .countDown_cont,
.main_countdown_timer .countDown_cont {
  display: flex;
  color: #ffffff;
}
.countdown_timer .countDown_interval_cont,
.main_countdown_timer .countDown_interval_cont {
  display: flex;
  justify-content: space-around;
  width: auto;
}
.countdown_timer .countDown_interval_cont:nth-child(n+1):not(:last-child),
.main_countdown_timer .countDown_interval_cont:nth-child(n+1):not(:last-child) {
  margin-right: 1em;
}
.countdown_timer .countDown_interval_basic_cont,
.main_countdown_timer .countDown_interval_basic_cont {
  display: flex;
  flex-direction: column;
}
.countdown_timer .countDown_interval_basic_cont_description,
.main_countdown_timer .countDown_interval_basic_cont_description {
  display: none;
}
.countdown_timer .countDown_digit_cont,
.main_countdown_timer .countDown_digit_cont {
  perspective: 3.2em;
  box-shadow: 0.1em 0.1em 0.1em rgba(0, 0, 0, 0.2);
  width: 1em;
  height: 1.6em;
  position: relative;
  line-height: 1.6em;
  font-size: 2em;
  font-weight: bold;
  border-radius: 0.08em;
  @include respond-to('medium'){
    font-size: 1.5em;
  }
}
.countdown_timer .countDown_digit_cont:nth-child(n+1):not(:last-child),
.main_countdown_timer .countDown_digit_cont:nth-child(n+1):not(:last-child) {
  margin-right: 0.05em;
}
.countdown_timer .countDown_digit_last_placeholder,
.countdown_timer .countDown_digit_new_placeholder, 
.main_countdown_timer .countDown_digit_last_placeholder,
.main_countdown_timer .countDown_digit_new_placeholder {
  position: absolute;
  left: 0;
  width: 100%;
  height: 50%;
  text-align: center;
  overflow: hidden;
}
.countdown_timer .countDown_digit_last_placeholder,
.main_countdown_timer .countDown_digit_last_placeholder {
  bottom: 0;
  background: #000000;
  border-radius: 0 0 0.08em 0.08em;
}
.countdown_timer .countDown_digit_last_placeholder_inner,
.main_countdown_timer .countDown_digit_last_placeholder_inner {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  bottom: 50%;
  position: absolute;
}
.countdown_timer .countDown_digit_new_placeholder,
.main_countdown_timer .countDown_digit_new_placeholder {
  top: 0;
  background: #000000;
  border-radius: 0.08em 0.08em 0 0;
}
.countdown_timer .countDown_digit_last_rotate,
.countdown_timer .countDown_digit_new_rotate,
.main_countdown_timer .countDown_digit_last_rotate,
.main_countdown_timer .countDown_digit_new_rotate {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 50%;
  font-weight: bold;
  position: absolute;
  top: 0;
  overflow: hidden;
  animation-duration: 0.4s;
  animation-timing-function: linear;
  border-radius: 0.08em 0.08em 0 0;
  animation-fill-mode: forwards;
  transform-origin: 100% 100%;
}
.countdown_timer .countDown_digit_last_rotate:after,
.countdown_timer .countDown_digit_new_rotate:after,
.main_countdown_timer .countDown_digit_last_rotate:after,
.main_countdown_timer .countDown_digit_new_rotate:after {
  content: "";
  position: absolute;
  z-index: -1;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  border-bottom: 0.01em solid rgba(0, 0, 0, 0.1);
}
.countdown_timer .countDown_digit_last_rotate,
.main_countdown_timer .countDown_digit_last_rotate {
  animation-name: countDown_flip_1;
  background: #000000;
}
.countdown_timer .countDown_digit_new_rotate,
.main_countdown_timer .countDown_digit_new_rotate {
  animation-name: countDown_flip_2;
  background: #000000;
}
.countdown_timer .countDown_digit_new_rotated,
.main_countdown_timer .countDown_digit_new_rotated {
  transform: rotateX(180deg);
  width: 100%;
  height: 100%;
}
.countdown_timer .countDown_digit_new_rotated_inner,
.main_countdown_timer .countDown_digit_new_rotated_inner {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  bottom: 50%;
  position: absolute;
}
@keyframes countDown_flip_1 {
  0% {
    transform: rotateX(0deg);
    z-index: 1;
  }
  100% {
    transform: rotateX(-180deg);
    z-index: 0;
  }
}
@keyframes countDown_flip_2 {
  0% {
    transform: rotateX(0deg);
    z-index: 0;
  }
  100% {
    transform: rotateX(-180deg);
    z-index: 1;
  }
}
