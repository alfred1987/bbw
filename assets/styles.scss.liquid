  }
}
.vertical-menu{
  display: inline;
}
.menu .vertical-menu li:hover .vertical-menu_submenu{
  visibility: visible;
  opacity: 1;
  transition: opacity 0.3s ease-in;
  pointer-events: all;
  z-index: 2000;
}
.menu .vertical-menu_submenu li:hover .vertical-menu_sub-submenu{
  visibility: visible;
  opacity: 1;
  transition: opacity 0.3s ease-in;
  pointer-events: all;
  z-index: 2000;
}
